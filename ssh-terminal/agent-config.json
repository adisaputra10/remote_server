{
  "agent_id": "vm-agent-001",
  "agent_name": "Production VM Agent",
  "server_url": "ws://localhost:8080/ws/agent",
  "auth_token": "secure-agent-token-2024",
  "environment": "production",
  "location": "datacenter-1",
  "capabilities": [
    "shell",
    "file_transfer",
    "system_info",
    "process_management"
  ],
  "logging": {
    "level": "info",
    "file": "logs/agent.log",
    "max_size_mb": 100,
    "max_files": 5
  },
  "security": {
    "allowed_commands": [
      "ls", "ps", "top", "whoami", "pwd", "cd", "cat", "tail", "head", "grep",
      "find", "df", "free", "uptime", "netstat", "ss", "systemctl", "service",
      "mkdir", "rmdir", "touch", "rm", "cp", "mv", "chmod", "chown",
      "curl", "wget", "ping", "telnet", "nslookup", "dig"
    ],
    "restricted_paths": [
      "/etc/shadow",
      "/etc/passwd",
      "/root/.ssh",
      "/home/*/.ssh"
    ],
    "max_command_length": 1000,
    "timeout_seconds": 300
  },
  "system": {
    "work_directory": "/tmp/goteleport",
    "shell": "/bin/bash",
    "user": "goteleport",
    "max_sessions": 10
  },
  "network": {
    "retry_attempts": 3,
    "retry_delay_seconds": 5,
    "heartbeat_interval_seconds": 30,
    "reconnect_on_disconnect": true
  },
  "features": {
    "enable_file_transfer": true,
    "enable_port_forwarding": false,
    "enable_x11_forwarding": false,
    "enable_session_recording": true
  }
}
