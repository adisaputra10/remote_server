# Docker Environment Configuration
# Copy this file to .env and modify as needed

# Authentication
TUNNEL_TOKEN=change-this-secure-token

# Agent Configuration  
AGENT_ID=docker-agent

# SSH Server Configuration
SSH_USER=testuser
SSH_PASSWORD=secure-password

# Optional: Relay server hostname (for external access)
RELAY_HOST=localhost

# Optional: Custom ports
RELAY_PORT=8443
SSH_PORT=2222
CLIENT_PORT=3333
